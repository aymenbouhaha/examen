{% extends 'layout.html.twig'%}

{% block title%}
    Affichage
{% endblock%}

{% block body%}
    <div class="alert alert-info">Nombre de Pfe par entreprise</div>
    {% for key in entreprise %}
        <div class="card text-white bg-primary mb-3" style="max-width: 18rem">
            <div class="card-body">
                <h5 class="card-title">{{ key.designation }}</h5>
                <p class="card-text">{{ key.pFEs.count() }} Pfe(s)</p>
            </div>
        </div>
    {% endfor%}
{% endblock%}